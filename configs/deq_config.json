{
        "setting": {
                "purpose": "pruning",
                "root_path": "./experiment/",
                "tiff_path": "/deq_acc6_selfsupervised_tiff/",
                "save_folder": "deq_acc6_selfsupervised_tiff",
                "mode": "train",
                "method": "Pruning_Finetuning",
                "acceleration_rate": 6,
                "dataset_type": "RealMeasurement",
                "du_weightsharing": true,
                "data_split_ratio": [0.9, 0.05, 0.05],
                "trainable_parameter": true
        },
        "pruning": {
                "//comment": "module: CNNBlock & EDSR & DEQ & DU & PNP & ISTANET & VARNET",
                "module": ["DEQ"],
                "module_path_deq": "./pretrained_models/deq_acc6.pt",
                "module_path_e2evarnet": "./pretrained_models/e2evarnet_acc6.pt",
                "module_path_varnet": "./pretrained_models/varnet_acc6.pt",
                "module_loading": true,
                "usage_of_gt": false,
                "//comment": "random, l1, lamp, XslimX, group_norm, Xgroup_slX, taylor",
                "importance": ["l1"],
                "data_fidelity": true,
                "fine_tuning": true,
                "fine_tune_with_testset": true,
                "student_teacher": true,
                "//commenct": "school & selfsupervised & supervised",
                "fine_tune_loss_type": "selfsupervised",
                "ch_sparsity": [0.4, 0.3, 0.2, 0.1, 0.05],
                "iterative_steps": 1
        },

        "rei": {
                "sigma_rei": 0.1,
                "tau": 1e-2,
                "alpha_req": 1.0
        },


        "dataset": {
                "synthetic": {
                        "sigma": 18,

                        "P": 1966,
                        "theta": 10,
                        "translation": [0, 0],
                        "rotate": 0,
                        "scale": 0
                },

                "root_path": "../../dataset/dataset/",
                "mask_type": "cartesian",
                "mask_fold": 3,
                "input_snr": 40,
                "multi_coil": true
        },

        "method": {
                "lambda_": 3.5,
                "loss_regis_mse": 0,
                "loss_regis_dice": 0,
                "loss_regis_ncc": 1,

                "loss_recon_consensus": 1.0,

                "proposed": {
                        "regis_batch": 1,
                        "recon_batch": 1,

                        "is_optimize_regis": true
                }
        },

        "module": {
                "regis": {
                         "nf_enc": [16, 32, 32, 32],
                         "nf_dec": [32, 32, 32, 32, 32, 16, 16]

                },

                "recon": {
                        "recon_net": "EDSR",

                        "EDSR": {
                                "n_resblocks": 13,
                                "n_feats": 64,
                                "res_scale": 1
                        },
			"//comment": "mu: 0.5, gamma:2.3, alpha: 0.9",
                        "iteration_k": 4,
                        "mu_list": [0.5],
                        "gamma_list": [5.0],
                        "alpha_list": [0.2],
                        "is_trainable_mu" : false,
                        "is_trainable_gamma" : false,
                        "is_trainable_alpha" : false,
                        "recon_module_type": "red"

                },
                "DEQ": {
                        "max_iter": 100,
                        "tol": 1e-4
                }
        },

        "train": {
                "batch_size":1,

                "num_workers": 0,
                "verbose_batch": 10,
                "train_epoch": 150,
                "checkpoint_epoch": 30,
                "tensorboard_batch": 50,

                "recon_lr": 1e-4,
                "regis_lr": 5e-4,

                "recon_loss": "smooth_l1",
                "regis_loss": null
        },

        "test": {
                "recon_checkpoint": "recon_model/best_valid_psnr.pt",
                "regis_checkpoint": "regis_model/best_valid_psnr.pt",

                "is_save_mat": false,
                "desc": ""

        }
}
